        <div class="misc-body">
            <a href="javascript:void(0)" class="misc-body-item introduction-qr-code hm-dropdown-shadow" style="display:none">
                <img src="/web/image/wechart-qr.png" alt="关注百度统计微信">
            </a>
        </div>

        <div class="main">

            <div id="report-wrapper" class="content clearfix">
                <div id="content-header-wrapper">
                    <div id="content-header" class="content-header">
                    </div>
                </div>

                <div class="page-tip" id="PageTip" style="display:none">
                    <a class="tip-close open trackable" memo="{id:'PageTip',type:'page-tip',global:0}" onclick="$(this).parent().hide();" href="javascript:void(0);"></a>
                    <div class="text"></div>
                </div>
                <div class="page-tip" id="ConditionalPageTip" style="display:none">
                    <a class="tip-close open trackable" memo="{id:'ConditionalPageTip',type:'page-tip',global:0}" onclick="$(this).parent().hide();" href="javascript:void(0);"></a>
                    <div class="text"></div>
                </div>


                <div class="main-content">
                    <div class="main-content-header">
                        <div class="title">百度推广</div>
                    </div>
                    <div id="pro-summary" class="section"><table><tbody><tr><td><div class="empty">最近两日暂无消费。</div></td></tr></tbody></table></div>
                </div>

                <div class="main-content clearfix">
                    <div class="main-content-header">
                        <div class="title">站点与子目录</div>
                        <div class="fav-filter">
                            <a class="fav-filter-all fav-filter-selected" href="#">全部</a>
                            <a class="fav-filter-favorite" href="#"></a>
                        </div>
                        <div id="search-container" class="search-container"><span class="search-btn">&nbsp;</span><span class="search-close" style="display: none;">&nbsp;</span><input type="text" class="search-input" placeholder="请输入..."></div>
