            }
        };

        $("#event-monitor-operation").click(function () {
            $(".nav-wrapper").addClass("hide");
            $(".main").css("margin-left", "51px");
            $(".simple-nav-wrapper").removeClass("hide");
        });
        $("#unfold").click(function () {
            $(".nav-wrapper").removeClass("hide");
            $(".main").css("margin-left", "181px");
            $(".simple-nav-wrapper").addClass("hide");
        })

        $("#main-menu>li>a").click(function () {
            var dd = $(this).next("ul").css('display');
            if (dd === "none") {
                $(this).next("ul").css('display', 'block');
            }
            if (dd === "block") {
                $(this).next("ul").css('display', 'none');
            }
        });


        $(".simple-nav>li>a").mouseenter(function () {
            $(this).next("ul").removeClass("hide");
            $(this).parent("li").addClass("cur");
        }).mouseleave(function () {
            $(this).next("ul").addClass("hide");
            $(this).parent("li").removeClass("cur");
        });

        $("#date-select-bar>a").click(function () {
            $(this).addClass("cur").siblings("a").removeClass("cur");
        })

        var mainMenu = document.getElementById("main-menu");
        var screenHeight = window.innerHeight
        mainMenu.style.height = screenHeight - 80 + "px";
    </script>
    <script src="~/js/re.js"></script>
</body>
</html>
